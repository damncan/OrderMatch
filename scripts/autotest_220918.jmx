          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Rradom generate Buy or Sell order (B for Buy, S for Sell)
Random random = new Random();
String [] buyOrSellArr = [&quot;B&quot;, &quot;S&quot;];
int select = random.nextInt(buyOrSellArr.length);
log.info(&quot;Random order generated: &quot; + buyOrSellArr[select]);
props.setProperty(&quot;randomBuyOrSell&quot;, buyOrSellArr[select]);

// Random generate (M for Market Order, L for Limit Order)
String [] orderTypeArr = [&quot;M&quot;, &quot;L&quot;];
select = random.nextInt(buyOrSellArr.length);
log.info(&quot;Random order type generated: &quot; + orderTypeArr[select]);
props.setProperty(&quot;randomOrderType&quot;, orderTypeArr[select]);

// Random generate quantity
int randomQuantity = generateRandomInt(1, 10);
log.info(&quot;Random quantity generated: &quot; + randomQuantity);
props.setProperty(&quot;randomQuantity&quot;, String.valueOf(randomQuantity));

// Random generate price
if(orderTypeArr[select].equals(&quot;L&quot;)){
	int randomPrice = generateRandomInt(100, 200);
	log.info(&quot;Random price generated: &quot; + randomPrice);
	props.setProperty(&quot;randomPrice&quot;, String.valueOf(randomPrice));
} else {
	props.setProperty(&quot;randomPrice&quot;, &quot;&quot;);
}

int generateRandomInt(int min, int max) {
	Random random = new Random();
	return random.nextInt((max - min) + 1) + min;
}</stringProp>
